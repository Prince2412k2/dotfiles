if command -v mise &> /dev/null; then
  eval "$(mise activate bash)"
fi

source /usr/share/nvm/init-nvm.sh 


# If inside a project venv and .postactivate exists, source it
if [[ -n "$VIRTUAL_ENV" && -f "$(dirname "$VIRTUAL_ENV")/.venvrc" ]]; then
    source "$(dirname "$VIRTUAL_ENV")/.venvrc"
fi

if command -v fzf &> /dev/null; then
  source ~/.local/share/fzf/completion.bash
  source ~/.local/share/fzf/key-bindings.bash
fi
 
 

# Enable autoenv
export AUTOENV_HOME="$HOME/.autoenv"
[[ -f "$AUTOENV_HOME/activate.sh" ]] && source "$AUTOENV_HOME/activate.sh"



eval "$(starship init bash)"

if command -v zoxide &> /dev/null; then
  eval "$(zoxide init bash)"
fi


